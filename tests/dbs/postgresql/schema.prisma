datasource postgres {
  url      = "postgresql://prisma:prisma@localhost:5433/prisma"
  provider = "postgres"
}

generator photonjs {
  provider = "photonjs"
  output   = "./@generated/photon"
}

generator testutils {
  provider = "prisma-test-utils"
  output   = "./@generated/prisma-test-utils"
}

model User {
  id       String  @id @default(cuid())
  name     String
  email    String
  isActive Boolean
}